<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <rule>
        <!--拦截页面以/_开头的数据,作为转发给前端使用-->
        <from>^/(\w+)/_([\w\d])+\.jhtml</from>
        <!-- to: 真实的URL -->
        <to type="forward">/$1.jhtml?param=$2</to>
    </rule>
    <rule>
        <from>^/aboutus.?(.*)$</from>
        <to>/index/aboutus.jhtml</to>
    </rule>
    <rule>
        <from>^/contact.?(.*)$</from>
        <to>/index/contact.jhtml</to>
    </rule>
    <rule>
        <from>^/helps.?(.*)$</from>
        <to>/index/helps.jhtml</to>
    </rule>
    <rule>
        <from>^/vip.?(.*)$</from>
        <to>/index/vip.jhtml</to>
    </rule>
    <rule>
        <from>^/questions.?(.*)$</from>
        <to>/index/questions.jhtml</to>
    </rule>
    <rule>
        <from>^/agreements.?(.*)$</from>
        <to>/index/agreements.jhtml</to>
    </rule>
    <rule>
        <from>^/group.?(.*)$</from>
        <to>/index/group.jhtml</to>
    </rule>
</urlrewrite>